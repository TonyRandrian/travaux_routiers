# Configuration Firebase
# Copiez ce fichier en .env et remplissez avec vos valeurs Firebase
REACT_APP_FIREBASE_API_KEY=your_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Configuration API Backend
REACT_APP_API_URL=http://localhost:3000

# Configuration Carte
REACT_APP_TILE_SERVER_URL=http://localhost:8080/tile/{z}/{x}/{y}.png
REACT_APP_MAP_CENTER_LAT=-18.8792
REACT_APP_MAP_CENTER_LNG=47.5079
REACT_APP_MAP_ZOOM=13
